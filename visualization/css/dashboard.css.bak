#pieResult div {
	zoom: 0.95;
}
.gadget { 
	width: 500px;
	height: 400px; 
	min-width: 300px;
	min-height: 200px;
	margin: 0 0.5em 1em 0;
	border: 2px solid #D8D8D8; 
	-webkit-border-radius: 6px;
	-moz-border-radius: 6px;
	border-radius: 6px;
	position:absolute !important;
	background-color: white;
}
.gadget-header{
	margin: 3px;
	background: #F2F2F2;
	-webkit-border-top-left-radius: 6px;
	-webkit-border-top-right-radius: 6px;
	-moz-border-radius-topleft: 6px;
	-moz-border-radius-topright: 6px;
	border-top-left-radius: 6px;
	border-top-right-radius: 6px;
	padding-bottom: 4px; 
	padding-left: 3px;
	padding-top: 3px;
	padding-right: 3px;
	cursor: move;
}
.gadget-content{
    padding-left:5px;
	padding-right:5px;
	background:white;
}
.gadget-close {
  float: right;
  font-size: 20px;
  font-weight: bold;
  line-height: 20px;
  color: #000000;
  text-shadow: 0 1px 0 #ffffff;
  opacity: 0.2;
  filter: alpha(opacity=20);
}
.gadget-close:hover,
.gadget-close:focus {
  color: #000000;
  text-decoration: none;
  cursor: pointer;
  opacity: 0.4;
  filter: alpha(opacity=40);
}

.gadget-edit {
  float: right;
  font-size: 20px;
  font-weight: bold;
  line-height: 20px;
  color: #000000;
  text-shadow: 0 1px 0 #ffffff;
  opacity: 0.2;
  filter: alpha(opacity=20);
}
.gadget-edit:hover,
.gadget-edit:focus {
  color: #000000;
  text-decoration: none;
  cursor: pointer;
  opacity: 0.4;
  filter: alpha(opacity=40);
}
.tabContentTitle {
	font-size: 15;
	font-weight: bold;
	padding-bottom: 5px;
}
.columnSelection {
	padding-left: 20px;
}
.welcome {
	color: white;
	padding-top: 10px;
	padding-right: 20px;	
}
#save {
	margin-left: 20px;
}